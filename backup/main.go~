package main

import (
    "fmt"
    "math"
)

func Sqrt(x float64) float64 {
    z := 10.0
    actualRoot := math.Sqrt(x)
    fmt.Println(actualRoot)
    for n := 1; z/actualRoot != 1.0; z, n = z - (z*z - x)/(2*z), n+1 {
        fmt.Println("z", n, math.Abs(z/actualRoot))
    }
    return z
}

func main() {
     for i:=0; i<10000000; i++{
         fmt.Println(i)
     }
}
